<?xml version="1.0"?>
<form>
    <field formid="firstName" label="contactus.firstname.label" type="string" mandatory="true" binding="custom.firstName" max-length="50" regexp="^[a-zA-Z]{2,10}$"/>
    <field formid="lastName" label="contactus.lastname.label" type="string" mandatory="true" binding="custom.lastName" max-length="50" regexp="^[a-zA-Z]{2,10}$"/>
    <field formid="address" label="customeraddress.addressid.label" type="string" mandatory="true" binding="custom.address" max-length="50" />
    <field formid="email" label="contactus.email.label" type="string" mandatory="true" binding="custom.email" max-length="50" 
        regexp="^[\w-\.]{1,}\@([\da-zA-Z-]{1,}\.){1,}[\da-zA-Z-]{2,6}$" 
    	parse-error="contactus.email.parse-error" 
    	value-error="This email address already exists!" />
    <field formid="city" label="resource.city" type="string" mandatory="true" binding="custom.city" max-length="50" />
    <field formid="country" label="resource.country" type="string" mandatory="true" binding="custom.country">
        <options>
            <option optionid="prompt" label="resource.select" value="" />
            <option optionid="us" label="country.us" value="us" />
            <option optionid="ca" label="country.ca" value="ca" />
            <option optionid="jp" label="country.jp" value="jp" />
            <option optionid="fr" label="country.fr" value="fr" />
            <option optionid="it" label="country.it" value="it" />
            <option optionid="cn" label="country.cn" value="cn" />
            <option optionid="uk" label="country.gb" value="gb" />
        </options>
    </field>
